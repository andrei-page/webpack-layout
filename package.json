  "description": "Webpack 5 template with Pug, Sass and Typescript.",
  "main": "src/js/index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack serve --config webpack.dev.js",
    "watch": "NODE_ENV=production webpack --config webpack.dev.js  --progress --watch",
    "build": "NODE_ENV=production webpack --config webpack.prod.js  --progress",
    "lint": "eslint 'src/js/**'",
    "deploy": "NODE_ENV=production webpack --config webpack.prod.js --progress && surge -d website.surge.sh build",
    "pretty": "prettier --write 'src/js/**'"
  },
  "dependencies": {
    "jquery": "^3.6.1",
